String[] st1;
int[] fq;
while(sc.hasNextLine())
{
s=sc.nextLine()+" ";
l=s.length();
for(i=0;i<l;i++)
{
if(s.charAt(i)==' ')
{
st[p]=s.substring(j,i);
j=i+1;
p=p+1;
}
}
for(i=0;i<c;i++)
{
for(k=0;k<c;k++)
{
if(st[i].equals(st[k])
f++;
}
fq[ct]=f;
st1[ct]=st[i];
for(int a=0;a<st1.length();a++)
